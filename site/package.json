{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for site goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "generate-signature-data": "node lib/generate-signature-data.js && pnpm prettier app/components/signature-data.ts --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "devDependencies": {
